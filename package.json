  "description": "Front-end for rosie web module",
  "main": "src/js/main.js",
  "scripts": {
    "lint": "eslint --fix --ext='[.js, .jsx]' src/js",
    "test": "mocha --compilers js:babel-register --recursive 'test/**/*.js*'",
